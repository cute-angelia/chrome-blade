{
  "name": "LaoShiJi",
  "version": "1.0.4",
  "manifest_version": 2,
  "description": "秋名山老司机福利-注定［福利］最强，是油猴子福利插件的集合体，正义与邪恶的化身，光明与豪迈的绝缘体，卑鄙与狡诈的融合体，凶残与毒辣的亲和体，勇敢与无畏的半导体",
  "homepage_url": "http://lsj.aacc.in/",
  "icons": {
    "16": "src/img/icons/icon16.png",
    "48": "src/img/icons/icon48.png",
    "128": "src/img/icons/icon128.png"
  },
  "background": {
    "scripts": [
      "src/js/background.js"
    ],
    "persistent": true
  },
  "options_page": "src/option/index.html",
  "browser_action": {
    "default_icon": "src/img/icons/icon48.png",
    "default_title": "LaoShiJi"
  },
  "permissions": [
    "cookies",
    "clipboardRead",
    "clipboardWrite",
    "storage",
    "webRequest",
    "notifications",
    "*://*/*",
    "tabs"
  ],
  "content_scripts": [{
    "matches": [
      "http://*/*",
      "https://*/*",
      "file://*/*"
    ],
    "js": [
      "src/js/vendor/jquery.min.js"
    ],
    "css": [
      "src/css/inject.css"
    ]
  }, {
    "matches": [
      "http://*/*",
      "https://*/*",
      "file://*/*"
    ],
    "js": [
      "src/js/vendor/jquery.min.js",
      "src/js/vendor/underscore.min.js",
      "src/js/inject.js"
    ]
  }],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}